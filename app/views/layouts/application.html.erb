<!DOCTYPE html>
<html>
<head>
  <title>BodyShop</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">BodyShop</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li><%= link_to 'Clients', clients_path %></li>
          <li><%= link_to 'Cars', cars_path %></li>
          <li><%= link_to 'Orders', orders_path %></li>
          <li><%= link_to 'Operations', operations_path %></li>
          <li><%= link_to 'Invoices', invoices_path %></li>
          <li><%= link_to 'DetailCategory', details_categorys_path %></li>
          <li><%= link_to 'ProviderList', providers_lists_path %></li>
          <li><%= link_to 'ListOperations', list_operations_path %></li>
          <li><%= link_to 'Masters', masters_index_path %></li>
        </ul
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
<%= yield %>

</body>
</html>
